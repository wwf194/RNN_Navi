{
    "Initialize":[
        {
            "Type": "BuildObj",
            "Args": [
                {
                    "ParamPath": "&param.PlaceCells",
                    "MountPath": "&PlaceCells",
                    "ModulePath": "Models.PlaceCells2D"
                },
                {
                    "ParamPath": "&param.model",
                    "MountPath": "&model",
                    "ModulePath": "Models.RNNLIF"                    
                }
            ]
        },
        {
            "Type": "FunctionCall",
            "Args": [
                "&PlaceCells.InitFromParam",
                "&model.InitFromParam",
                "&SetTrajectory2ModelInputMethod",
                "&SetTrajectory2ModelOutputMethod",
            ]
        },
    ],
    "PlaceCells":{
        "Num": "$^param.model.Neurons.Num // 4",
        "XYs":{
            "Initialize": {
                "Method": "FromFunctionCall",
                "Functions": [
                    [
                        "&^object.world.GetArenaByIndex(0).GenerateRandomInternalXYs",
                        ["$PlaceCells.Num"]
                    ],
                    [
                        "&^object.agent.PlaceCells.ReceiveXYs",
                        ["__PreviousFunctionOutput__"]
                    ]
                ]
            }
        }
    },
    "model":"$^param.model"
}